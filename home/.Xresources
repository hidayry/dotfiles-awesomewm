Xft.antialias:	1
Xft.hinting:	1
Xft.autohint:	0
Xft.hintstyle:	hintslight
Xft.rgba:	rgb
Xft.lcdfilter:	lcddefault

! Urxvt
URxvt*font: xft:JetBrainsMono Nerd Font:size=15,xft:Symbols Nerd Font-6
URxvt*letterSpace: 0
URxvt*scrollBar: false
URxvt*internalBorder: 16

! window padding
st.borderpx: 20

!-- values between 0.1  - 1.0 --! 

st.alpha: 0.8
st.font: JetBrainsMono Nerd Font:style:medium:pixelsize=15

!-- colors --!

*.foreground:   #a5b6cf
*.background:   #0d0f18
*.cursorColor:  #151720

! special
!*.foreground:   #edeff0
!*.background:   #0c0e0f

! black
*.color0:       #232526
*.color8:       #2c2e2f

! red
*.color1:       #df5b61
